{
    "name": "chatbot-frontend",
    "license": "Apache-2.0",
    "version": "0.1.0",
    "type": "module",
    "engines": {
        "node": ">=24.0.0"
    },
    "scripts": {
        "dev": "vite",
        "dev:tauri": "tauri dev",
        "build": "tsc -b && vite build",
        "build:tauri": "tauri build",
        "lint": "eslint . --fix",
        "lint:check": "eslint .",
        "format": "prettier --write \"**.{ts,tsx,css,json,mjs}\"",
        "format:check": "prettier --check \"**.{ts,tsx,css,json,mjs}\"",
        "preview": "vite preview",
        "tauri": "tauri",
        "update-capabilities": "node scripts/update-capabilities.mjs",
        "android:init": "tauri android init",
        "android:dev": "npm run update-capabilities && tauri android dev",
        "android:build": "npm run update-capabilities && tauri android build",
        "ios:init": "tauri ios init",
        "ios:dev": "npm run update-capabilities && tauri ios dev",
        "ios:build": "npm run update-capabilities && tauri ios build"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.3.7",
        "@mui/material": "^7.3.7",
        "@reduxjs/toolkit": "^2.11.2",
        "@tauri-apps/api": "^2.9.1",
        "@tauri-apps/plugin-http": "^2.5.6",
        "@tauri-apps/plugin-shell": "^2.3.4",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "react-redux": "^9.2.0",
        "react-router-dom": "^7.12.0",
        "sharp": "^0.34.5"
    },
    "devDependencies": {
        "@eslint/compat": "^2.0.1",
        "@eslint/js": "^9.39.2",
        "@tauri-apps/cli": "^2.9.6",
        "@types/react": "^19.2.9",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.2",
        "eslint": "^9.39.2",
        "eslint-config-airbnb-extended": "^3.0.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.5",
        "globals": "^17.0.0",
        "prettier": "^3.8.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.53.1",
        "vite": "^7.3.1"
    }
}
